class TwoSum_1 {

    public int[] twoSum(int[] nums, int target) {
        for (int i = 0; i < nums.length; i++) {
            for (int j = 0; j < nums.length; j++) {
                if ((nums[i] + nums[j]) == target && i != j) {
                    int[] results = new int[2];
                    results[0] = i;
                    results[1] = j;
                    return results;
                }
            }

        }
        return null;

    }


}
